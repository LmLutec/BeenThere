<%= form_for @user, url: login_path do |f| %>
    <h1>Log In</h1>
    <%= f.text_field :email, placeholder: "Email" %>
    <%= f.password_field :password, placeholder: "Password" %>
    <%= f.submit "Log In" %>
<% end %>
</br>

<div id= "google_login">
    <%= link_to('Log in with Google!', '/auth/google_oauth2') %></br></br>
</div>

<div id="new_user">
    <h3>New User?</h3> 
    <%= link_to 'Create new account', root_path %>
</div>