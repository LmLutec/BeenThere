<h1>Log In</h1>
<%= form_for @user, url: login_path do |f| %>
    Email: <%= f.text_field :email %>
    Password: <%= f.password_field :password %>
    <%= f.submit "Log In" %>
<% end %>
</br>
<%= link_to('Log in with Google!', '/auth/google_oauth2') %></br></br>


<div id="new_user">
    <h3>New User?</h3> <%= link_to 'Create new account', root_path %>
</div>